{
  "name": "movie-rating-app",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register test/**/*.js*",
    "dev": "./node_modules/.bin/webpack-dev-server --content-base src --inline --hot",
    "mkdir": "mkdir -p build",
    "build": "npm run clean && npm run mkdir && npm run build:html && npm run build:css && npm run build:js",
    "serve": "node server.js",
    "http": "http-server build/",
    "watch": "npm run watch:html & npm run watch:css & npm run watch:js",
    "clean": "rm -rf build",
    "build:html": "npm run clean:html && cp src/index.html build/",
    "watch:html": "npm run build:html && chokidar src/index.html -c 'npm run build:html'",
    "clean:html": "rm -f build/index.html",
    "build:css": "npm run clean:css && cp src/css/style.css build/",
    "clean:css": "rm -f build/style.css",
    "watch:css": "npm run build:css && chokidar src/css/style.css -c 'npm run build:css'",
    "build:js": "npm run clean:js && webpack",
    "watch:js": "npm run build:js && webpack --watch",
    "clean:js": "rm -f build/$npm_package_name.$npm_package_version.js build/$npm_package_name.$npm_package_version.js.map"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "auth0-lock": "^10.2.2",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.11.10",
    "pg": "^6.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-star-rating-component": "^1.2.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chokidar-cli": "^1.2.0",
    "http-server": "^0.9.0",
    "redux-logger": "^2.6.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
